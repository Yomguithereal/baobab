{
  "name": "baobab",
  "version": "2.3.4",
  "description": "JavaScript persistent data tree with cursors.",
  "main": "./dist/baobab.js",
  "module": "./es/baobab.js",
  "jsnext:main": "./es/baobab.js",
  "types": "./index.d.ts",
  "dependencies": {
    "emmett": "^3.1.1"
  },
  "devDependencies": {
    "@yomguithereal/eslint-config": "^1.1.1",
    "add-banner": "^0.1.0",
    "async": "^1.2.1",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "benchmark": "^1.0.0",
    "browserify": "^13.3.0",
    "cross-env": "^3.1.4",
    "eslint": "^1.1.0",
    "lodash": "^3.6.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.0.1",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "benchmark": "babel-node benchmark.js",
    "check": "npm test && npm run lint && npm run build",
    "lint": "eslint -c eslint.config.js ./src ./test",
    "prepublish": "npm run clean && npm run build",
    "test": "cross-env BABEL_ENV=commonjs mocha -R spec --compilers js:babel-register ./test/endpoint.js",
    "clean": "rimraf dist es build",
    "build:umd": "mkdirp build && cross-env BABEL_ENV=commonjs browserify ./src/baobab.js -t babelify -s Baobab -o ./build/baobab.js && uglifyjs ./build/baobab.js -c -m -o ./build/baobab.min.js && node ./scripts/banner.js",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:es && npm run build:commonjs && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yomguithereal/baobab.git"
  },
  "keywords": [
    "cursors",
    "atom",
    "tree",
    "react"
  ],
  "author": {
    "name": "Guillaume Plique",
    "url": "http://github.com/Yomguithereal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yomguithereal/baobab/issues"
  },
  "homepage": "https://github.com/Yomguithereal/baobab"
}